{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "plugins": [],
  "settings": {},
  "ecmaFeatures": {
    "jsx": true,
    "arrowFunctions": true,
    "blockBindings": true,
    "defaultParams": true,
    "modules": true,
    "restParams": true,
    "binaryLiterals": true,
    "forOf": true,
    "generators": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralDuplicateProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "octalLiterals": true,
    "regexUFlag": true,
    "regexYFlag": true,
    "superInFunctions": true,
    "templateStrings": true,
    "unicodeCodePointEscapes": true,
    "globalReturn": true
  },
  "rules": {
    "no-underscore-dangle": 1,
    "no-unused-vars": 1,
    "no-multi-spaces": 1,
    "key-spacing": 1,
    "no-return-assign": 1,
    "consistent-return": 1,
    "no-shadow": 1,
    "comma-dangle": 1,
    "no-use-before-define": 1,
    "no-empty": 1,
    "new-parens": 1,
    "no-cond-assign": 1,
    "quotes": [1, "single", "avoid-escape"],
    "camelcase": 1,
    "new-cap": [1, { "capIsNew": false }],
    "no-undef": 1,
    "semi": 1,
    "eol-last": 1,
    "max-len": [1, 120, 4],
    "no-var": 1,
    "space-before-function-paren": [1, "never"],
    "strict": [1, "global"]
  }
}
